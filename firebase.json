{
  "hosting": {
    "public": "public",
    "ignore": [
       "firebase.json",
       "**/.*",
       "**/node_modules/**"
     ],
     "headers": [ {
      "source" : "**/*.@(eot|otf|ttf|ttc|woff|font|css|jpg|jpeg|gif|png)",
      "headers" : [ {
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      } ]
      }, {
      "source" : "**/*.@(eot|otf|ttf|ttc|woff|font|css|jpg|jpeg|gif|png|js)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=30672000"
      } ]
      }, {
      "source" : "404.html",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=300"
      } ]
    } ]
  }
}
